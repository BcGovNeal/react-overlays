{
  "name": "react-overlays",
  "version": "1.0.0-beta.2",
  "description": "Utilities for creating robust overlay components",
  "author": "Jason Quense <monastic.panic@gmail.com>",
  "repository": "react-bootstrap/react-overlays",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "files": [
    "lib"
  ],
  "keywords": [
    "react-overlays",
    "react-component",
    "react",
    "overlay",
    "react-component",
    "tooltip",
    "bootstrap",
    "popover",
    "modal"
  ],
  "scripts": {
    "build:esm": "babel src -d lib --env-name esm && file-butler mjs lib -d lib && rimraf lib/*.js",
    "build:cjs": "babel src -d lib",
    "build": "rimraf lib && npm run build:esm && npm run build:cjs",
    "build-examples": "rimraf examples/static && webpack --config webpack/docs.config.js",
    "examples": "webpack-dev-server --config webpack/examples.config.js --hot --port 3000 --progress",
    "lint": "eslint examples src test *.js",
    "release": "release",
    "tdd": "cross-env NODE_ENV=test karma start",
    "test": "npm run lint && npm run testonly",
    "testonly": "npm run tdd -- --single-run"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "dom-helpers": "^3.3.1",
    "popper.js": "^1.14.3",
    "prop-types": "^15.6.1",
    "prop-types-extra": "^1.1.0",
    "react-transition-group": "^2.3.1",
    "warning": "^4.0.1"
  },
  "peerDependencies": {
    "react": "^>=16.3.0",
    "react-dom": "^>=16.3.0"
  },
  "devDependencies": {
    "@4c/file-butler": "^1.0.2",
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.49",
    "@babel/polyfill": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "@monastic.panic/component-playground": "^3.2.2",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "chai": "^4.1.2",
    "codecov": "^3.0.2",
    "component-metadata-loader": "^4.0.0",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-react": "^7.8.2",
    "jquery": "^3.3.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon-chai": "^1.3.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "less": "^2.7.2",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "node-libs-browser": "^2.1.0",
    "raw-loader": "^0.5.1",
    "react": "^16.4.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.0",
    "release-script": "^1.0.2",
    "rimraf": "^2.6.2",
    "simulant": "^0.2.2",
    "sinon": "^5.0.10",
    "sinon-chai": "^3.1.0",
    "style-loader": "^0.21.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  }
}

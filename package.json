{
  "name": "react-overlays",
  "version": "1.0.0-beta.12",
  "description": "Utilities for creating robust overlay components",
  "author": "Jason Quense <monastic.panic@gmail.com>",
  "repository": "react-bootstrap/react-overlays",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/es/index.js",
  "files": [
    "lib"
  ],
  "keywords": [
    "react-overlays",
    "react-component",
    "react",
    "overlay",
    "react-component",
    "tooltip",
    "bootstrap",
    "popover",
    "modal"
  ],
  "scripts": {
    "build:esm": "babel src -d lib/es --env-name esm",
    "build:cjs": "babel src -d lib --delete-dir-on-start",
    "build": "rimraf lib && yarn build:cjs && yarn build:esm",
    "build:examples": "webpack --config webpack/docs.config.js --mode production",
    "deploy-docs": "yarn --cwd www build && gh-pages -d www/public",
    "prepublishOnly": "yarn build && file-butler alt-publish-root",
    "examples": "yarn --cwd www start",
    "lint": "eslint examples src test *.js",
    "release": "np --contents lib",
    "tdd": "cross-env NODE_ENV=test karma start",
    "test": "yarn lint && yarn testonly",
    "testonly": "yarn tdd --single-run"
  },
  "dependencies": {
    "@react-bootstrap/react-popper": "^1.1.0",
    "classnames": "^2.2.6",
    "dom-helpers": "^3.3.1",
    "prop-types": "^15.6.2",
    "prop-types-extra": "^1.1.0",
    "react-context-toolbox": "^1.1.0",
    "warning": "^4.0.1"
  },
  "peerDependencies": {
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0"
  },
  "devDependencies": {
    "@4c/file-butler": "^1.1.1",
    "@babel/cli": "^7.0.0-beta.52",
    "@babel/core": "^7.0.0-beta.52",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.52",
    "@babel/polyfill": "^7.0.0-beta.52",
    "@babel/preset-env": "^7.0.0-beta.52",
    "@babel/preset-react": "^7.0.0-beta.52",
    "@monastic.panic/component-playground": "^3.2.2",
    "@monastic.panic/enzyme-adapter-react-16": "^1.2.4",
    "babel-eslint": "^8.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "chai": "^4.1.2",
    "cherry-pick": "^0.2.1",
    "codecov": "^3.0.2",
    "cross-env": "^5.2.0",
    "enzyme": "^3.1.0",
    "eslint": "^4.19.1",
    "eslint-plugin-mocha": "^5.1.0",
    "eslint-plugin-react": "^7.10.0",
    "gh-pages": "^1.2.0",
    "jquery": "^3.3.1",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon-chai": "^1.3.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "np": "^3.0.4",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.1",
    "react-live": "^1.11.0",
    "react-transition-group": "^2.4.0",
    "rimraf": "^2.6.2",
    "simulant": "^0.2.2",
    "sinon": "^6.1.3",
    "sinon-chai": "^3.2.0",
    "webpack": "^4.16.3",
    "webpack-atoms": "^7.0.1"
  }
}

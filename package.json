{
  "name": "react-overlays",
  "version": "1.0.0-beta.6",
  "description": "Utilities for creating robust overlay components",
  "author": "Jason Quense <monastic.panic@gmail.com>",
  "repository": "react-bootstrap/react-overlays",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "files": [
    "lib"
  ],
  "keywords": [
    "react-overlays",
    "react-component",
    "react",
    "overlay",
    "react-component",
    "tooltip",
    "bootstrap",
    "popover",
    "modal"
  ],
  "scripts": {
    "build:esm": "babel src -d lib --env-name esm && file-butler mjs lib -d lib && rimraf lib/*.js",
    "build:cjs": "babel src -d lib",
    "build": "rimraf lib && npm run build:esm && npm run build:cjs",
    "build:examples": "webpack --config webpack/docs.config.js --mode production",
    "deploy-docs": "npm run build:examples && gh-pages -d examples/dist",
    "prepublishOnly": "npm run build",
    "examples": "webpack-dev-server --config webpack/examples.config.js --port 3000 --mode development",
    "lint": "eslint examples src test *.js",
    "release": "release",
    "tdd": "cross-env NODE_ENV=test karma start",
    "test": "npm run lint && npm run testonly",
    "testonly": "npm run tdd -- --single-run"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "dom-helpers": "^3.3.1",
    "popper.js": "^1.14.3",
    "prop-types": "^15.6.2",
    "prop-types-extra": "^1.1.0",
    "react-transition-group": "^2.4.0",
    "warning": "^4.0.1"
  },
  "peerDependencies": {
    "react": "^>=16.3.0",
    "react-dom": "^>=16.3.0"
  },
  "devDependencies": {
    "@4c/file-butler": "^1.0.2",
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.51",
    "@babel/polyfill": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@monastic.panic/component-playground": "^3.2.2",
    "babel-eslint": "^8.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "chai": "^4.1.2",
    "codecov": "^3.0.2",
    "component-metadata-loader": "^4.0.0",
    "cross-env": "^5.2.0",
    "gh-pages": "^1.2.0",
    "eslint": "^4.19.1",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-react": "^7.10.0",
    "jquery": "^3.3.1",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon-chai": "^1.3.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.1",
    "release-script": "^1.0.2",
    "rimraf": "^2.6.2",
    "simulant": "^0.2.2",
    "sinon": "^6.0.1",
    "sinon-chai": "^3.2.0",
    "webpack": "^4.14.0",
    "webpack-atoms": "^7.0.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
